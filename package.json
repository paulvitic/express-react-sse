{
  "name": "app-with-express-no-stress",
  "version": "1.0.0",
  "description": "App with express-no-stress",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "docker-compose -f .docker/docker-compose.yml up -d && nodemon server/index.ts",
    "dev:debug": "nodemon --exec 'yarn compile && node -r ts-node/register --inspect-brk' server/index.ts",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest --testPathPattern=test\\.ts$ --passWithNoTests",
    "test:debug": "jest --inspect-brk --exit",
    "test:integration": "docker-compose -f .docker/docker-compose.yml up -d && jest --testPathPattern=intg\\.ts$ --passWithNoTests",
    "test:functional": "docker-compose -f .docker/docker-compose.yml up -d && jest --testPathPattern=func\\.ts$ --passWithNoTests"
  },
  "dependencies": {
    "amqplib": "^0.5.5",
    "axios": "^0.19.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-list-middleware": "^1.0.1",
    "express-session": "^1.17.0",
    "pg": "^7.17.0",
    "connect-pg-simple": "^6.1.0",
    "swagger-express-middleware": "^2.0.4",
    "winston": "^3.2.1",
    "fp-ts": "^1.19.5"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/axios": "^0.14.0",
    "@types/connect-redis": "^0.0.13",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/pg": "^7.14.0",
    "@types/winston": "^2.4.4",
    "@types/jest": "^24.0.23",
    "typescript": "^3.7.5",
    "jest": "^24.9.0",
    "ts-lint": "^4.5.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.4.1",
    "nodemon": "^1.19.4",
    "shelljs": "^0.8.5",
    "supertest": "^4.0.2"
  }
}
